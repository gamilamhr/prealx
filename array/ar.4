#include <stdio.h>
/**
* main - Entry point of the program
* Description:number of values in an array and display them in reverse order.
*
* Return: 0 (success)
**/
 void main()
{
 int arr[100];
 int i, j, n, count =0;

 printf("Input the number of elements to be stored in the array :\n");
 scanf("%d", &n);
 printf("Input %d elements in the array", n);
 for(i = 0; i < n; i++)
 {
    printf("element - %d : ", i);
    scanf("%d", &arr[i]);
}
    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
                break;
            }
        }
    }
     printf("Total number of duplicate elements found in the array is: %d\n", count);

    return 0;
}

#include<stdio.h>
int main()

{
    int num1 ,num2;
    {
    printf("enter number 1: ");
    scanf("%d" ,&num1);
    }
    
  {
    printf("enter number 2: ");
    scanf("%d" ,&num2);
    }
    if(num1 > num2)
    {
    printf("%d is a maximum", num1);
    }
    
    if(num2 > num1)
    {
    printf("%d is a maximum", num2);
    }
     
     if(num1 == num2)
    {
        printf("both are equal");
    }
    return 0;
}
    

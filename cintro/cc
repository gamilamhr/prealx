#include <stdio.h>

void main(void)
{
    int age=50;
    float salary;
    
    printf("Hello world\n");
    printf("your age is %d\n",age);
    printf("please give me your salary:" );
    scanf("%f" , &salary);
    printf("your slary is %7.2f\n",salary);
    printf("koko\ttroto\tsoso\n");
    printf("your age is %f\n");
    printf("good bye !!!\n");
}
